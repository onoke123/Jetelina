apino,sql,subquery
ji307,"insert into ftest (jt_id,ftest_name,ftest_sex,ftest_age,ftest_ave,jetelina_delete_flg) values({jt_id},'{ftest_name}','{ftest_sex}',{ftest_age},{ftest_ave},{jetelina_delete_flg})",""
ju308,"update ftest set ftest_name='{ftest_name}',ftest_sex='{ftest_sex}',ftest_age={ftest_age},ftest_ave={ftest_ave}","where jt_id={jt_id}"
jd309,"update ftest set jetelina_delete_flg=1","where jt_id={jt_id}"
js310,"select ftest.jt_id,ftest.ftest_name,ftest.ftest_sex,ftest.ftest_age,ftest.ftest_ave from ftest as ftest","ignore"
js311,"select ftest.jt_id,ftest.ftest_sex,ftest.ftest_ave from ftest as ftest","where ftest_ave<0.4"
