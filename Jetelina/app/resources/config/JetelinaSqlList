ju405,"update ftest2 set ftest2_name='{ftest2_name}',ftest2_sex='{ftest2_sex}',ftest2_age={ftest2_age},ftest2_ave={ftest2_ave}","where ftest2_jt_id={jt_id}"
jd406,"update ftest2 set jetelina_delete_flg=1","where ftest2_jt_id={jt_id}"
js407,"select ftest.ftest_jt_id,ftest2.ftest2_sex from ftest as ftest,ftest2 as ftest2","where ftest.ftest_name = ftest2.ftest2_name"
