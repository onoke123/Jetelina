ju402,"update ftest set ftest_name='{ftest_name}',ftest_sex='{ftest_sex}',ftest_age={ftest_age},ftest_ave={ftest_ave}","where ftest_jt_id={jt_id}"
jd403,"update ftest set jetelina_delete_flg=1","where ftest_jt_id={jt_id}"
